<%- include('./partials/head.ejs') %>
<%- include('./partials/header.ejs') %>


    <form action="/index" method="POST" enctype="multipart/form-data">
        <div class="form-floating mb-3">
            <input type="text" class="form-control" name="name" id="floatingInput" placeholder="Spreadsheet Name">
        </div>
        <div class="form-floating mb-3">
            <input type="text" class="form-control" name="description" id="floatingInput" placeholder="Description">
        </div>
        <div class="form-floating mb-3">
            <input type="text" class="form-control" name="plImpact" id="floatingInput" placeholder="Financial Impact $">
        </div>
        <div class="form-floating mb-3">
            <input type="text" class="form-control" name="source" id="floatingInput" placeholder="Data Source">
        </div>
        <div class="form-floating mb-3">
            <input type="text" class="form-control" name="location" id="floatingInput" placeholder="Location">
        </div>
        <div class="form-floating mb-3">
            <input type="text" class="form-control" name="reportingUnit" id="floatingInput" placeholder="Reporting Unit">
        </div>

        <div class="form-group">

            <label for="excel">Upload File</label>
            <input type="file" name="excel" id="excel"  >
        </div>


<br/>
        <input type="submit" class="btn btn-success" value="Submit" style="margin-left: 1   0px;" />




    </form>

